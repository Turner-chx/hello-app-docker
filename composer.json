{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "alaczi/twig-country-extension": "v1.2.0",
        "beberlei/doctrineextensions": "v1.3.0",
        "doctrine/doctrine-migrations-bundle": "3.0.2",
        "doctrine/persistence": "1.3.8",
        "friendsofsymfony/ckeditor-bundle": "2.2.0",
        "friendsofsymfony/oauth-server-bundle": "1.6.2",
        "friendsofsymfony/rest-bundle": "3.0.4",
        "guzzlehttp/guzzle": "6.5.5",
        "helios-ag/fm-elfinder-bundle": "10.0.4",
        "myclabs/php-enum": "1.7.2",
        "oomphinc/composer-installers-extender": "2.0.0",
        "phpoffice/phpspreadsheet": "^1.22",
        "sensio/framework-extra-bundle": "v5.6.1",
        "sonata-project/admin-bundle": "3.83.0",
        "sonata-project/doctrine-orm-admin-bundle": "3.26.0",
        "sonata-project/exporter": "2.5.0",
        "sonata-project/intl-bundle": "2.10.0",
        "sonata-project/translation-bundle": "2.7.0",
        "sonata-project/user-bundle": "4.10.1",
        "stof/doctrine-extensions-bundle": "v1.5.0",
        "studio-42/elfinder": "2.1.57",
        "symfony/acl-bundle": "v2.0.0",
        "symfony/apache-pack": "v1.0.1",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "v1.11.0",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "v3.6.0",
        "symfony/process": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/swiftmailer-bundle": "v3.5.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "tetranz/select2entity-bundle": "2.*",
        "twig/extensions": "v1.5.4",
        "twig/twig": "v2.14.1",
        "vich/uploader-bundle": "1.16.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/stopwatch": "4.4.*",
        "symfony/var-dumper": "^4.4",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/web-server-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "1.1.6",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "elfinder:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
